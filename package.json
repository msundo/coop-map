    },
    "devDependencies": {
        "autoprefixer": "^7.2.5",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "del-cli": "^1.1.0",
        "babel-preset-env": "^1.6.1",
        "copy-webpack-plugin": "^4.5.0",
        "cross-env": "^5.1.3",
        "css-loader": "^0.28.9",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.6",
        "handlebars": "^4.0.11",
        "handlebars-loader": "^1.6.0",
