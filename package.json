{
    "name": "simple-frontend",
    "version": "1.0.0",
    "main": "dist",
    "author": "Addition",
    "private": true,
    "standard": true,
    "license": "UNLICENSED",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dashboard webpack-dev-server --content-base src/ --host 0.0.0.0",
        "build": "yarn install --silent --loglevel=error && del ./dist && cross-env NODE_ENV=production webpack -p --progress --display=errors-only",
        "test": "standard",
        "stats": "webpack -p --json > stats.json"
    },
    "files": [
        "polyfills.js",
        "main.js",
        "main.css"
    ],
    "dependencies": {
